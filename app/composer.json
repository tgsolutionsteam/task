{
  "name": "jmsilvadev/task",
  "type": "project",
  "description": "API Task",
  "version": "0.0.1",
  "license": "proprietary",
  "authors": [
    {
      "name": "josuesilva",
      "email": "jmsilvadev@gmail.com",
      "role": "developer"
    }
  ],
  "require": {
    "php": ">=7.4",
    "ext-fileinfo": "*",
    "ext-json": "*",
    "ext-pdo": "*",
    "ext-phalcon": "^4",
    "cocur/slugify": "4.0.0",
    "robmorgan/phinx": "^0.11.6",
    "league/fractal": "^0.19.2",
    "ext-redis": "*",
    "cache/redis-adapter": "^1",
    "milo/xml-rpc": "^3.0"
  },
  "require-dev": {
    "codeception/c3": "2.5.*",
    "codeception/codeception": "^4.1",
    "codeception/module-asserts": "^1.2",
    "codeception/module-phalcon": "^1.0.0",
    "codeception/module-phpbrowser": "^1.0.0",
    "codeception/module-rest": "^1.2",
    "ergebnis/composer-normalize": "^2.5",
    "fzaninotto/faker": "^1.9",
    "hoa/console": "^3.17",
    "mockery/mockery": "^1.3",
    "phalcon/ide-stubs": "^4",
    "phploc/phploc": "^5.0",
    "phpmd/phpmd": "^2.8",
    "phpmetrics/phpmetrics": "^2.6",
    "squizlabs/php_codesniffer": "^3.5"
  },
  "config": {
    "platform": {
      "php": "7.4"
    },
    "vendor-dir": "vendor"
  },
  "autoload": {
    "psr-4": {
      "App\\Controllers\\": "src/Controllers/",
      "App\\Helpers\\": "src/Helpers/",
      "App\\HttpClient\\": "src/HttpClient/",
      "App\\Models\\": "src/Models/",
      "App\\Middlewares\\": "src/Middlewares/",
      "App\\Providers\\": "src/Providers/",
      "App\\Services\\": "src/Services/",
      "App\\Repositories\\": "src/Repositories/",
      "App\\Traits\\": "src/Traits/",
      "App\\Errors\\": "src/Errors/",
      "App\\Factories\\": "src/Factories/",
      "App\\Observers\\": "src/Observers/",
      "App\\DTO\\": "src/DTO/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  }
}
